{
  "expo": {
    "name": "Hostel Wiz",
    "description": "An application for finding the best hostels and apartments in Ghana",
    "slug": "hostelWiz",
    "primaryColor": "#E7C654",
    "platforms": [
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.hostelWiz.app",
      "googleServicesFile": "./google-services.json"
    }
  }
}
